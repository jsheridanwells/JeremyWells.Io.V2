    "clean-css": "^5.3.2",
    "dotenv": "^16.0.3",
    "eleventy-gist": "^1.1.0",
    "jest": "^29.5.0",
    "luxon": "^3.3.0",
    "markdown-it-attrs": "^4.1.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.0",
    "terser": "^5.18.0"
  }
